ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/BRYXBanner" "${PODS_CONFIGURATION_BUILD_DIR}/ChromaColorPicker" "${PODS_CONFIGURATION_BUILD_DIR}/Hue" "${PODS_CONFIGURATION_BUILD_DIR}/SwiftMQTT" "${PODS_CONFIGURATION_BUILD_DIR}/SwiftyButton"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/BRYXBanner/BRYXBanner.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/ChromaColorPicker/ChromaColorPicker.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Hue/Hue.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/SwiftMQTT/SwiftMQTT.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/SwiftyButton/SwiftyButton.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "BRYXBanner" -framework "ChromaColorPicker" -framework "Hue" -framework "SwiftMQTT" -framework "SwiftyButton"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
